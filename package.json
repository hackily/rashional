{
  "name": "rashional",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node server/app.js",
    "test": "mocha",
    "babel-node": "babel-node --presets=es2015,react ./server/app.js",
    "build": "browserify ./frontend/client/main.js -o ./public/scripts/bundle.js",
    "build-server": "babel --presets=es2015,react ./frontend/components --out-file ./frontend/components/babelified.js",
    "build-all": "npm run build && npm run build-server",
    "watch": "watchify ./frontend/client/main.js -o public/scripts/bundle.js -dv",
    "docs": "apidoc -i server/ -o apidoc/",
    "preversion": "npm test",
    "version": "git add package.json && git commit -m \"version bump\"",
    "postversion": "git push"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ],
      [
        "scssify"
      ]
    ]
  },
  "engines": {
    "node": "7.5.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.26.0",
    "babel-plugin-object-assign": "^1.2.1",
    "body-parser": "~1.13.2",
    "browserify": "^14.4.0",
    "browserify-middleware": "^7.1.0",
    "bulma": "^0.5.2",
    "chart.js": "^1.0.2",
    "clarifai": "^2.3.2",
    "cookie-parser": "~1.3.5",
    "cssify": "^1.0.3",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-http-proxy": "^0.6.0",
    "express-session": "^1.12.1",
    "http-proxy-middleware": "^0.9.1",
    "https-proxy-agent": "^1.0.0",
    "lodash": "^4.17.4",
    "materialize-css": "^0.100.2",
    "mongoose": "^4.11.10",
    "passport": ">= 0.0.0",
    "passport-oauth2-middleware": "*",
    "passport-predix-oauth": "0.1.15",
    "react": "^15.6.1",
    "react-d3-basic": "^1.6.11",
    "react-dom": "^15.6.1",
    "react-easy-chart": "^0.1.13",
    "react-i18next": "^5.3.0",
    "react-router": "^4.2.0",
    "request": "~2.67.0",
    "sassify": "^2.0.0",
    "url": "~0.11.0",
    "winston": "^2.3.1"
  },
  "author": "",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "chai": "^3.5.0",
    "mocha": "^2.5.3",
    "watchify": "^3.9.0"
  }
}
